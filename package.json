{
  "name": "nlp-article-analyzer-web-tool",
  "version": "1.0.0",
  "description": "a sentimental analysis web tool to extract information from texts ",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest",
    "start": "node src/server/index.js",
    "devStart": "nodemon src/server/server.js",
    "build": "webpack",
    "build-prod": "webpack --config config/webpack.prod.js",
    "build-dev": "webpack-dev-server  --config config/webpack.dev.js --open",
    "dev": "webpack serve --mode development --env development",
    "serve": "webpack serve --config config/webpack.dev.js --progress --profile --watch --content-base src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aimogue/NLP-Article-Analyzer-Web-Tool.git"
  },
  "keywords": [
    "web",
    "tool",
    "nlp",
    "sentimental",
    "analysis"
  ],
  "author": "ahmed ibrahim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aimogue/NLP-Article-Analyzer-Web-Tool/issues"
  },
  "homepage": "https://github.com/aimogue/NLP-Article-Analyzer-Web-Tool#readme",
  "dependencies": {
    "express": "^4.17.1",
    "node": "^15.4.0",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "webpack-dev-server": "^3.11.2"
  }
